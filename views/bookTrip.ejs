<style>
input[type=text]:focus {
	outline: none;
	box-shadow: 0px 1px 0px 0px #008489;
}

body {
	font-family: Circular, "Helvetica Neue", Helvetica, Arial, sans-serif;
	font-size: 14px;
	line-height: 1.43;
	color: #484848;
	background-color: #fff;
	-webkit-font-smoothing: antialiased;
	-moz-osx-font-smoothing: grayscale;
}

.dropdown-content {
	display: none;
	position: absolute;
	background-color: white;
	min-width: 160px;
	box-shadow: 0px 8px 16px 0px rgba(0, 0, 0, 0.2);
}

.dropdown-content a {
	color: black;
	padding: 12px 16px;
	text-decoration: none;
	display: block;
}

.dropdown-content a:hover {
	background-color: #f1f1f1
}

.dropdown:hover .dropdown-content {
	display: block;
}

.dropdown:hover .dropbtn {
	background-color: #3e8e41;
}

select:focus {
	outline: none;
	box-shadow: 0px 1px 0px 0px #008489;
}

a:focus {
	outline: none
}

.req {
	font-size: 150%;
	color: red;
}

button:focus {
	outline: none
}

#map {
	height: 100%;
}

.accordion-panel__content {
	overflow: hidden;
	padding: 5px;
	width: 100%;
}

.accordion-panel--collapsed {
	max-height: 0;
	padding: 0;
}

.accordion-panel--expanded {
	max-height: 2048px;
	opacity: 1;
	overflow: visible;
	transition: opacity 0.3s, max-height 0.8s, padding 0s ease 0s;
}

sup {
	font-size: 75%;
	line-height: 0;
	position: relative;
	vertical-align: baseline;
}

.accordion_checkout__section {
	overflow: hidden;
	padding: 5px;
	width: 100%;
}

.light-gray {
	color: #bbb;
}

.select {
	padding-top: 8px;
	padding-bottom: 8px;
	padding-right: 2em;
	padding-left: 10px;
	height: 50px;
	width: 100%;
	transition: border-color 0.2s;
	border: 1px solid #aaa;
	border-radius: 2px;
	background-color: #fff;
	color: #484848;
	line-height: normal;
}

.photo {
	backface-visibility: hidden;
	position: relative;
	display: inline-block;
	vertical-align: bottom;
	overflow: hidden;
	background-color: #bbb;
}

.btn {
	display: inline-block;
	margin-bottom: 0;
	-webkit-border-radius: 4px;
	-moz-border-radius: 4px;
	border-radius: 4px;
	border: 1px solid;
	text-align: center;
	vertical-align: middle;
	font-weight: bold;
	line-height: 1.43;
	-webkit-user-select: none;
	-moz-user-select: none;
	-ms-user-select: none;
	user-select: none;
	white-space: nowrap;
	cursor: pointer;
}

.billing_table {
	width: 100%;
	max-width: 100%;
	border-collapse: collapse;
	border-spacing: 0;
	display: table;
	border-color: grey;
}

tbody {
	display: table-row-group;
	vertical-align: middle;
	border-color: inherit;
}

tr {
	display: table-row;
	vertical-align: inherit;
	border-color: inherit;
}

.summary {
	background: transparent;
	padding: 0 0 15px;
	font-size: 16px;
	float: none;
	margin-left: auto;
	margin-right: auto;
}

.summary .panel-body:first-child {
	padding-top: 30px;
	border-top: 0;
}

.total_price {
	border: 0;
	padding: 0;
	width: 100%;
	background: transparent;
	text-align: start;
	cursor: default;
	pointer-events: none;
}

input[type="checkbox"] {
	position: relative;
	-webkit-appearance: none;
	height: 1.25em;
	width: 1.25em;
	margin-bottom: -0.25em;
	margin-right: 5px;
	vertical-align: top;
}

.star {
	/* padding: 1px; */
	color: #ddd;
	font-size: 30px;
	text-shadow: .05em .05em #aaa;
	list-style-type: none;
	display: inline-block;
	cursor: pointer;
}

.filled {
	color: #fd0;
}
</style>
