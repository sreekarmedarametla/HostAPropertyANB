	<script>
		listing = "<%=JSON.stringify(property)%>"
		no_of_guests = "<%=JSON.stringify(no_of_guests)%>"
	</script>
	<script>
		
		$('#next')
				.on(
						'click',
						function(e) {
							

							$('#payment')
									.toggleClass(
											'accordion-panel--collapsed accordion-panel--expanded');
							$('#trip')
									.toggleClass(
											'accordion-panel--collapsed accordion-panel--expanded');
							$('#checkout').show();
							$('#edit').show();
							$('#acc_trip').addClass('light-gray');
							$('#acc_payment').removeClass('light-gray');
						});
		$('#edit')
				.on(
						'click',
						function(e) {

							$('#payment')
									.toggleClass(
											'accordion-panel--collapsed accordion-panel--expanded');
							$('#trip')
									.toggleClass(
											'accordion-panel--collapsed accordion-panel--expanded');
							$('#checkout').hide();
							$('#edit').hide();
							$('#acc_trip').removeClass('light-gray');
							$('#acc_payment').addClass('light-gray');
						});

		/* $('#continue')
		.on(
				'click',
				function(e) {

					
				    $('#nights').val(nights);
				});
		 */
	</script>


