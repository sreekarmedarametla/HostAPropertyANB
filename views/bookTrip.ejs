<body ng-app="trip" ng-controller="trip" ng-init="getPropReviews();">
	<div style="z-index: 1000; display: block; position: relative;">
		<nav class="navbar navbar-default "
			style="background-color: transparent; background: transparent; border-color: transparent;">
			<div class="container-fluid">
				<div class="navbar-header">
					<a class="navbar-brand" href="/home"
						style="border-right: 1px solid #dce0e0;"> <img
						src="https://a1.muscache.com/airbnb/static/icons/android-icon-192x192-c0465f9f0380893768972a31a614b670.png"
						alt="some_text"
						style="height: 30px; font-weight: normal; font-size: inherit; background-color: transparent; color: inherit; top: -2px; font-family:"HelveticaNeue";font-size: 14px;    padding: 4px 12px;     ">
					</a>

				</div>
				<ul class="nav navbar-nav pull-right" style="z-index: 100000;"
					style="overflow: hidden; font-family: Circular,"HelveticaNeue",Helvetica; top: -2px;font-size: 14px;padding: 4px 12px;"-webkit-font-smoothing:antialiased;">

					<li ng-if="profile.status  === 0"><a href=""
						ng-click="becomeHost()">Become a Host</a></li>
					<li ng-if="profile.status  === 1"><a href=""><span
							style="color: red">Become a Host (Pending)</span></a>
					<li ng-if="profile.status  === 2"><a href="/hostlistings">Host</a>
					<li><a href="/trips">Trips</a>
					<li class="dropdown"><a href="/profile"> <img width="28"
							height="28" class="img-circle dropbtn" ng-src="{{imageUrl}}"
							style="border-color: #c4c4c4; border: 2px solid #c4c4c4;">
					</a>

						<div class="dropdown-content">
							<label> <a href="/logout">Logout</a>
							</label>
						</div></li>

				</ul>

			</div>
		</nav>
	</div>
	<div class="container">
		<div class="row" style="height: 30px"></div>

		<div class="row">


			<form ng-submit="checkout(avgrating);" method="post” name="
				checkout” data-toggle="validator">

				<div class="col-md-7 form-group" ng-click="resetErrors();">



					<div class="accordion-panel" data-panel-name="AboutYourTrip">

						<div class="row">
							<div class="col-sm-12">
								<div class="alert alert-danger" ng-if="no_of_guests>property.guests">
									<strong>Select lesser number of guests</strong>
								</div>
							</div>
						</div>
						
						
						
						<div class="row">
							<div id="acc_trip" class="col-md-10 text-left">
								<div style="font-size: 24px; font-weight: normal; float: left;">1.
									About Your Trip</div>

							</div>
							<div id="edit" class="col-md-2 text-right"
								style="line-height: 26px; display: none; font-size: 16px;">
								<a href="#"
									style="color: #008489; text-decoration: none; background: transparent;">
									<span>Edit</span>
								</a>
							</div>
						</div>
						<div style="margin-bottom: 12px;"></div>
						<div id="trip"
							class="accordion-panel__content accordion-panel--expanded">
							<div style="font-size: 16px;">
								<div style="margin-bottom: 24px;">
									<div class="host-message" style="font-size: 16px;">
										<br>

										<div style="display: inline-block; vertical-align: middle;">
											<img width="28" height="28" class="img-circle"
												ng-src="{{hostImageUrl}}"
												style="border-color: #c4c4c4; border: 2px solid #c4c4c4;">
											<span>Your host, <%=property.lname%></span>

										</div>

									</div>

								</div>
								<div style="margin-bottom: 24px;">
									<div class="guest_details">
										<span>Who’s coming?</span>
										<div class="row" style="margin-top: 6px;">
											<div class="col-md-12">
												<div
													style="display: block; width: 50%; position: relative; vertical-align: bottom;">
													<select class="form-control" id="how-many-guests"
														placeholder="1 Guest" ng-model="no_of_guests" required>
														<option value="1">1 Guest</option>
														<option value="2">2 Guests</option>
														<option value="3">3 Guests</option>
														<option value="4">4 Guests</option>
														<option value="5">5 Guests</option>
														<option value="6">6 Guests</option>
														<option value="7">7 Guests</option>
														<option value="8">8 Guests</option>
														<option value="9">9 Guests</option>
														<option value="10">10 Guests</option>
														<option value="11">11 Guests</option>
														<option value="12">12 Guests</option>
														<option value="13">13 Guests</option>
														<option value="14">14 Guests</option>
														<option value="15">15 Guests</option>
														<option value="16">16+ Guests</option>
													</select>
												</div>
											</div>
										</div>
									</div>
								</div>



								<div style="margin-bottom: 42px;">
									<div>
										<label for="guest_input"
											style="display: block; padding-top: 9px; padding-bottom: 8px; line-height: normal;">
											<span>Say hello to your host and tell them why you’re
												coming:</span> <span></span>
										</label>
										<div class="guest_message">
											<div>
												<textarea name="message_host"
													placeholder="Visiting family or friends? Seeing the sights? This helps your host plan for your trip."
													rows="4"
													style="max-height: 1000px; padding-top: 10px; padding-bottom: 10px; resize: vertical; line-height: inherit; display: block; width: 100%"></textarea>
											</div>
										</div>
									</div>
									<div style="margin-top: 6px;">
										<div></div>
									</div>
								</div>
								<div style="margin-bottom: 36px;"></div>
							</div>
							<div class="row" style="margin-bottom: 12px; margin-top: 12px;">
								<div class="col-md-4">
									<button id="next" class="btn btn-large btn-primary" ng-disabled="no_of_guests>property.guests"
										type="button"
										style="height: 50px; border-color: #ff5a5f; background-color: #ff5a5f; color: #fff; width: 50%;">
										<span>Next</span>
									</button>
								</div>
							</div>


						</div>

					</div>



					
					<div class="accordion-panel" data-panel-name="Payment">
						<hr
							style="border-top-width: 2px; margin-top: 6px; margin-bottom: 18px; border-top: 1px solid #dce0e0;">
						<div class="row">
							<div id="acc_payment" class="col-md-10 text-left light-gray">
								<div style="font-size: 24px; font-weight: normal; float: left;">2.
									Payment</div>

							</div>
						</div>
						<hr
							style="border-top-width: 2px; margin-top: 18px; margin-bottom: 18px; border-top: 1px solid #dce0e0;">
						<div style="margin-bottom: 12px;"></div>
						<div id="payment"
							class="accordion-panel__content accordion-panel--collapsed"
							style="font-size: 16px;">
							<div id="selectors">
								<div>
									<div>
										<div class="row" style="margin-bottom: 24px;">
											<div class="col-md-6">

												<div>
													<label for="payment-country"
														style="display: block; padding-top: 9px; padding-bottom: 8px; line-height: normal; cursor: default;">
														<span>Billing country</span>
													</label>
													<div
														style="display: block; width: 100%; position: relative; vertical-align: bottom;">
														<select id="payment_country" name="payment_country"
															class="select">

															<option selected value="US">United States</option>
														</select>
													</div>

												</div>
											</div>
										</div>
									</div>

									<div>
										<div class="row">
											<div class="col-md-12">
												<label for="payment-type"
													style="display: block; padding-top: 9px; padding-bottom: 8px; line-height: normal; cursor: default;">
													<span>Payment type</span>
												</label>
											</div>
										</div>
										<div class="row" style="margin-bottom: 24px;">
											<div class="col-md-6">
												<div>
													<div
														style="display: block; width: 100%; position: relative; vertical-align: bottom;">
														<select id="payment_country" name="payment_country"
															class="select">

															<option selected value="0">Credit Card</option>
														</select>
													</div>
												</div>
											</div>
											<div class="col-md-6">
												<div class="logos">
													<div class="payment-logo visa active"></div>
													<div class="payment-logo master active"></div>
													<div class="payment-logo american_express active"></div>
													<div class="payment-logo discover active"></div>
													<div class="payment-logo paypal inactive"></div>
												</div>
											</div>
										</div>
									</div>

								</div>
							</div>

							<div>
								<div></div>
								<div>
									<div class="row" style="margin-bottom: 24px;">

										<div class="col-md-12">



											<div class="row">
												<div class="col-md-9">
													<label for="card_number"
														style="font-size: 16px; white-space: nowrap; display: block; padding-top: 9px; padding-bottom: 8px; line-height: normal; cursor: default;">
														<span>Card number <abbr class="req"
															title="required">*</abbr></span> <span>&nbsp;</span>
													</label>
													<div style="position: relative;">
														<input class="form-control" name="card_number"
															pattern="\d{16}" title="Card number should be 16 digits"
															ng-model="cardNumber" type="tel" aria-required="true"
															autocomplete="off"
															style="padding-right: 40px; height: 50px; font-size: 16px; display: block; padding-top: 8px; padding-bottom: 8px; padding-left: 10px; width: 100%;"
															required maxlength="16">


													</div>
												</div>
											</div>
											<div class="row">
												<div class="col-md-6">
													<label for="card_month"
														style="font-size: 16px; white-space: nowrap; display: block; padding-top: 9px; padding-bottom: 8px; line-height: normal; cursor: default;">
														<span>Expires on <abbr class="req" title="required">*</abbr></span>
														<span>&nbsp;</span>
													</label>
													<div class="row">
														<div class="col-md-6">
															<div
																style="display: block; width: 100%; position: relative; vertical-align: bottom;">
																<select id="card_month" class="form-control"
																	name="card_month" class="select" ng-model="month"
																	required>

																	<option value="">MM</option>
																	<option value="1">1</option>
																	<option value="2">2</option>
																	<option value="3">3</option>
																	<option value="4">4</option>
																	<option value="5">5</option>
																	<option value="6">6</option>
																	<option value="7">7</option>
																	<option value="8">8</option>
																	<option value="9">9</option>
																	<option value="10">10</option>
																	<option value="11">11</option>
																	<option value="12">12</option>
																</select>
															</div>
														</div>
														<div class="col-md-6">
															<div
																style="display: block; width: 100%; position: relative; vertical-align: bottom;">
																<select id="card_year" class="form-control"
																	name="card_year" class="select" ng-model="year"
																	required>

																	<option value="">YYYY</option>
																	<option value="2016">2016</option>
																	<option value="2017">2017</option>
																	<option value="2018">2018</option>
																	<option value="2019">2019</option>
																	<option value="2020">2020</option>
																	<option value="2021">2021</option>
																	<option value="2022">2022</option>
																	<option value="2023">2023</option>
																	<option value="2024">2024</option>
																	<option value="2025">2025</option>
																	<option value="2026">2026</option>
																	<option value="2027">2027</option>
																</select>
															</div>
														</div>
													</div>
												</div>
												<div class="col-md-3">
													<label for="card_cvv"
														style="font-size: 16px; white-space: nowrap; display: block; padding-top: 9px; padding-bottom: 8px; line-height: normal; cursor: default;">
														<span>Security code <abbr class="req"
															title="required">*</abbr></span> <span>&nbsp;</span>
													</label>
													<div style="position: relative;">
														<input ng-model="cvv" class="form-control" name="card_cvv"
															pattern="\d{3}" title="Enter 3 digit security code"
															type="tel" aria-required="true" autocomplete="off"
															maxlength="3"
															style="padding-right: 40px; height: 50px; font-size: 16px; display: block; padding-top: 8px; padding-bottom: 8px; padding-left: 10px; width: 100%;"
															required>
													</div>
												</div>
											</div>
										</div>



									</div>

									<div class="row" style="margin-bottom: 24px;">
										<div class="col-md-6">
											<label for="first_name"
												style="font-size: 16px; white-space: nowrap; display: block; padding-top: 9px; padding-bottom: 8px; line-height: normal; cursor: default;">
												<span>First name <abbr class="req" title="required">*</abbr></span>
												<span>&nbsp;</span>
											</label>
											<div style="position: relative;">
												<input type="text" name="first_name" class="form-control"
													ng-model="firstname" pattern="^[a-zA-Z]*$"
													title="First name should only contain upper and lower case alphabets"
													style="padding-right: 40px; height: 50px; font-size: 16px; display: block; padding-top: 8px; padding-bottom: 8px; padding-left: 10px; width: 100%;"
													required>
											</div>
										</div>
										<div class="col-md-6">
											<label for="last_name"
												style="font-size: 16px; white-space: nowrap; display: block; padding-top: 9px; padding-bottom: 8px; line-height: normal; cursor: default;">
												<span>Last name <abbr class="req" title="required">*</abbr></span>
												<span>&nbsp;</span>
											</label>
											<div style="position: relative;">
												<input type="text" class="form-control" name="last_name"
													pattern="^[a-zA-Z]*$"
													title="Last name should only contain upper and lower case alphabets"
													ng-model="lastname"
													style="padding-right: 40px; height: 50px; font-size: 16px; display: block; padding-top: 8px; padding-bottom: 8px; padding-left: 10px; width: 100%;"
													required>
											</div>
										</div>
									</div>

									<div class="row" style="margin-bottom: 24px;">

										<div class="col-md-4">
											<label for="postal_code"
												style="font-size: 16px; white-space: nowrap; display: block; padding-top: 9px; padding-bottom: 8px; line-height: normal; cursor: default;">
												<span>Postal code <abbr class="req" title="required">*</abbr></span>
												<span>&nbsp;</span>
											</label>
											<div style="position: relative;">
												<input type="text" class="form-control" name="postal_code"
