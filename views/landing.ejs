	<!-- Modal -->
	<div id="login" class="modal fade" role="dialog" tabindex='-1'>

		<div class="modal-dialog ">

			<!-- Modal content-->
			<div class="modal-content">
				<div class="modal-body form-horizontal">
					<div class="form-group has-feedback" style="margin-bottom: 6px;">
						<div class="row" style="height: 30px"></div>
						<div class="row">
							<div class="col-md-1"></div>
							<div class='col-md-10'>
								<div ng-hide="invalid_login" style="color: red;">
									<span class="glyphicon glyphicon-exclamation-sign"></span> <strong>Username/password
										incorrect.</strong>
								</div>
								<div ng-hide="val_mess" style="color: red;">
									<span class="glyphicon glyphicon-exclamation-sign"></span> <strong>{{message}}</strong>
								</div>
							</div>
						</div>
						<div class="row">
							<div class='col-md-1'></div>
							<div class='col-md-10'>
								<button class="btn icon-btn btn-block  btn-large"
									style="position: relative; padding-top: 20px !important; padding-bottom: 20px !important; border-color: #3B5998; background-color: #3B5998; color: #fff; padding-left: 27px !important; padding-right: 27px !important; font-weight: normal;"
									name="button" type="submit">
									<span><i class="fa fa-facebook-f"></i></span> <span
										class="text-container" style=""> Log in with Facebook </span>
								</button>
							</div>
						</div>

						<div class="row" style="height: 20px"></div>
						<div class="row">
							<div class="col-md-1"></div>
							<div class="col-sm-10">
								<button class="btn icon-btn btn-block  btn-large"
									style="position: relative; padding-top: 20px !important; padding-bottom: 20px !important; border-color: #c4c4c4; background: white; color: #484848; padding-left: 27px !important; padding-right: 27px !important; font-weight: normal;"
									name="button" type="submit">
									<span><i class="fa fa-google"></i></span> <span
										class="text-container" style=""> Log in with Google </span>
								</button>
							</div>
						</div>
						<div class="row" style="height: 20px"></div>
						<div
							style="width: 100%; text-align: center; line-height: 0.1em; margin: 10px 0 20px;">
							<span>or</span>
						</div>
						<form name="myForm">
							<div class="row">
								<div class="col-md-1"></div>
								<div class="col-sm-10" style="position: relative;">
									<div style="padding: 10px;"
										ng-class="{ 'has-error' :myForm.email.$touched &&  myForm.email.$invalid}">
										<input id='p_name' name='email' type="email"
											class='form-control input-lg' placeholder="Email Address"
											ng-model="email" required> <span
											class='glyphicon glyphicon-envelope form-control-feedback '
											area-hidden='true'></span>
										<p ng-show="myForm.email.$touched &&  myForm.email.$invalid"
											class="help-block">Should be an email</p>
									</div>
								</div>
							</div>


							<div class="row" style="height: 10px"></div>
							<div class="row">
								<div class="col-md-1"></div>
								<div class="col-sm-10">
									<div style="padding: 10px;"
										ng-class="{ 'has-error' :myForm.password.$touched &&  myForm.password.$invalid}">
										<input id='p_name' name='password' type='password'
											class='form-control input-lg' placeholder="Password"
											ng-model="password" required> <span
											class='glyphicon glyphicon-lock form-control-feedback'
											area-hidden='true'></span>
										<p
											ng-show="myForm.password.$touched &&  myForm.password.$invalid"
											class="help-block">Password is mandatory</p>

									</div>
								</div>
							</div>

							<div class="row" style="height: 10px"></div>
							<div class="row">
								<div class="col-sm-1"></div>
								<div class="col-sm-10">
									<input type="checkbox" name="remember_me" id="remember_me2"
										value="true" class="remember_me"> Remember me <a
										href="" class="forgot-password pull-right" data-from="email">Forgot
										password?</a>
								</div>
							</div>
							<div class="row" style="height: 10px"></div>
							<div class="row">
								<div class='col-md-1'></div>
								<div class='col-md-10'>
									<button class="btn icon-btn btn-block  btn-large"
										style="position: relative; padding-top: 20px !important; padding-bottom: 20px !important; border-color: #ff5a5f; background-color: #ff5a5f; color: #fff; padding-left: 27px !important; padding-right: 27px !important; font-weight: normal;"
										name="button" type="submit" ng-click="login();" ng-disabled="myForm.$invalid">
										<span class="text-container" style=""> Log in </span>
									</button>
								</div>
								<div class="row" style="height: 20px"></div>
							</div>
							<div class="row">
								<div class='col-md-1'></div>
								<div class='col-md-10'>
									<a href="/adminLogin" class="forgot-password pull-left"
										data-from="email">Login as Admin</a>
								</div>
							</div>
						</form>
					</div>
				</div>

				<div class="row" style="height: 10px"></div>
